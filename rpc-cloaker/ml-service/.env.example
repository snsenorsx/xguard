SERVICE_NAME=rpc-cloaker-ml
SERVICE_PORT=5000
SERVICE_HOST=0.0.0.0
DEBUG=true

# API Security
API_KEY=dev_ml_service_key

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/rpc_cloaker
REDIS_URL=redis://localhost:6379/1

# Model
MODEL_PATH=/app/models
MODEL_VERSION=latest
TRAINING_BATCH_SIZE=1000
TRAINING_INTERVAL_HOURS=6

# Features
FEATURE_WINDOW_HOURS=24
MIN_SAMPLES_FOR_TRAINING=10000

# MLflow
MLFLOW_TRACKING_URI=file:///app/mlruns
MLFLOW_EXPERIMENT_NAME=bot_detection

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Performance
MAX_WORKERS=2
REQUEST_TIMEOUT=30
