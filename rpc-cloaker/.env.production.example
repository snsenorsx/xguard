# Production Environment Variables
# Copy this file to .env.production and update values

# Domain Configuration
DOMAIN=your-domain.com
SSL_EMAIL=admin@your-domain.com

# Database
DB_PASSWORD=strong_production_password_here
POSTGRES_PASSWORD=strong_production_password_here

# Redis
REDIS_PASSWORD=strong_redis_password_here

# JWT
JWT_SECRET=very_long_random_string_for_jwt_secret_in_production

# ML Service
ML_SERVICE_API_KEY=strong_ml_service_api_key_here

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
DATADOG_API_KEY=your_datadog_api_key_here

# External Services
IPINFO_API_KEY=your_ipinfo_api_key
MAXMIND_LICENSE_KEY=your_maxmind_license_key